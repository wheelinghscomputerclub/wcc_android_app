<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whs"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/whs_app_icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light" >
        <activity
            android:name="com.example.whs.WHS"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.whs.News"
            android:label="@string/title_activity_news"
            android:parentActivityName="com.example/whs.WHS"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whsWHS" />
        </activity>
        <activity
            android:name="com.example.whs.Events"
            android:label="@string/title_activity_events"
            android:parentActivityName="com.example/whs.WHS"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whsWHS" />
        </activity>
        <activity
            android:name="com.example.whs.Announcements"
            android:label="@string/title_activity_announcements"
            android:parentActivityName="com.example/whs.WHS"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.whsWHS" />
        </activity>
        <activity
            android:name="com.example.whs.Read"
            android:label="@string/title_activity_read" >
        </activity>
        <activity
            android:name="com.example.whs.Navigate"
            android:label="@string/title_activity_navigate" >
        </activity>
        <activity
            android:name="com.example.whs.ChangeSettings"
            android:label="@string/title_activity_change_settings" >
        </activity>
        <activity
            android:name="com.example.whs.WHS_Home"
            android:label="@string/title_activity_whs__home" >
        </activity>
    </application>

</manifest>
